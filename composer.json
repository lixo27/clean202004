{
  "name": "lixo27/clean202004",
  "description": "Use this project as a way to quickly start any new laravel package.",
  "keywords": [
    "laravel"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/lixo27/clean202004/issues",
    "source": "https://github.com/lixo27/clean202004"
  },
  "authors": [
    {
      "name": "Thiago Senna",
      "email": "thiago@thremes.com.br"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-intl": "*",
    "laravel/framework": "^7.0"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.9.1"
  },
  "autoload": {
    "psr-4": {
      "Clean\\": "includes/src/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Clean\\Providers\\Customers\\CustomerServiceProvider",
        "Clean\\Providers\\Employees\\EmployeeServiceProvider",
        "Clean\\Providers\\Products\\ProductServiceProvider",
        "Clean\\Providers\\Sales\\SaleServiceProvider",
        "Clean\\Providers\\CleanServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
